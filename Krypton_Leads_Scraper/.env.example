# Ultimate Scraper Configuration
# Copy this file to .env and configure your settings

# Google Places API Key (optional but recommended)
# Get from: https://console.cloud.google.com/apis/credentials
GOOGLE_PLACES_API_KEY=your_api_key_here

# Proxy Configuration (optional)
# Add your proxy servers one per line
# PROXY_1=http://username:password@proxy1:port
# PROXY_2=http://username:password@proxy2:port

# Google Sheets Configuration (optional)
# Path to your Google service account credentials JSON file
GOOGLE_SHEETS_CREDENTIALS=credentials.json

# Scraper Configuration
MAX_BROWSER_INSTANCES=10
MAX_CONCURRENT_REQUESTS=15
DEFAULT_TIMEOUT=5
CACHE_DURATION_HOURS=6

# Rate Limiting (be respectful)
DELAY_BETWEEN_REQUESTS=0.5
MAX_RETRIES=5

# Performance Settings
ENABLE_CACHING=true
ENABLE_PROXY_ROTATION=false
ENABLE_USER_AGENT_ROTATION=true
ENABLE_API_FALLBACK=true